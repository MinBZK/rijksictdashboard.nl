{
  "name": "rid-publiek",
  "version": "0.0.0",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --max-warnings 0 && vue-tsc --noEmit",
    "build-only": "vite build",
    "build-dev": "vite build --mode development",
    "build-docker": "vite build --mode docker",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "generate-api-client": "openapi --input http://127.0.0.1:8000/openapi.json --useOptions --output ./src/api-new --client axios --exportSchemas true",
    "format-check": "prettier --check src/ '!src/api-new/**'",
    "format": "prettier --write 'src/**' '!src/api-new/**'"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "@unhead/vue": "1.11",
    "@vueuse/core": "^9.12.0",
    "@vueuse/router": "^9.12.0",
    "axios": "^1.1.3",
    "highcharts": "^11.0.0",
    "pinia": "^2.0.23",
    "primevue": "^3.31.0",
    "qs": "^6.11.2",
    "roboto-fontface": "*",
    "vue": "^3.3.4",
    "vue-matomo": "^4.2.0",
    "vue-router": "^4.1.5",
    "vuetify": "^3.3.1",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^16.11.68",
    "@types/webfontloader": "^1.0.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.28.0",
    "prettier": "^3.2.5",
    "sass": "^1.55.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.16.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^3.1.8",
    "vite-plugin-md": "^0.20.4",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^2.0.11"
  }
}
